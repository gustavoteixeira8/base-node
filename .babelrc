{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "9.1.0"
                }
            }
        ]
    ],
    "env": {
        "production": {
            "presets": [
                "minify"
            ]
        }
    },
    "plugins": [
        [
            "transform-object-rest-spread",
            {
                "useBuiltIns": true
            }
        ]
    ]
}
